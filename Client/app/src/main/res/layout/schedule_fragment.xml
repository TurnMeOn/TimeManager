<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    android:id="@+id/Sframe"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/SImage"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/backgroundimage3"
        android:scaleType="fitXY"/>
    <LinearLayout
        android:id="@+id/SLinear"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/STrelative"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal"
            android:background="#FFFFFF">
            <ImageView
                android:id="@+id/STLimageview"
                android:paddingLeft="3dp"
                android:paddingTop="3dp"
                android:layout_width="40dp"
                android:layout_height="match_parent"
                android:background="@drawable/default_head"/>
            <Spinner
                android:id="@+id/STMspinner"
                android:layout_width="100dp"
                android:layout_height="33dp"
                android:spinnerMode="dropdown"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"/>
            <ImageView
                android:id="@+id/STRimageview"
                android:layout_width="33dp"
                android:layout_height="33dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:background="@drawable/plus"/>
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/SMLinear"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#7fC3C7C0"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/SMmonth"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:maxEms="1"
                android:text=" 4月"/>
            <TextView
                android:id="@+id/SMmon"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:maxEms="2"
                android:text="周一 03日"/>
            <TextView
                android:id="@+id/SMtue"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:maxEms="2"
                android:text="周二 04日"/>
            <TextView
                android:id="@+id/SMwed"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:maxEms="2"
                android:text="周三 05日"/>
            <TextView
                android:id="@+id/SMthu"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:maxEms="2"
                android:text="周四 06日"/>
            <TextView
                android:id="@+id/SMfri"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:maxEms="2"
                android:text="周五 07日"/>
            <TextView
                android:id="@+id/SMsat"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:maxEms="2"
                android:text="周六 08日"/>
            <TextView
                android:id="@+id/SMsun"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:maxEms="2"
                android:text="周日 09日"/>
        </LinearLayout>
        <ScrollView
            android:id="@+id/Sscroll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:scrollbars="none">
            <LinearLayout
                android:id="@+id/SBlinear"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:baselineAligned="true">
                <LinearLayout
                    android:id="@+id/SBlinear0"
                    android:layout_width="match_parent"
                    android:layout_height="860dp"
                    android:layout_weight="1"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/t00"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textSize="20sp"
                        android:textColor="#ffffffff"
                        android:text="7"/>
                    <TextView
                        android:id="@+id/t01"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textSize="20sp"
                        android:textColor="#ffffffff"
                        android:text="8"/>
                    <TextView
                        android:id="@+id/t02"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textSize="20sp"
                        android:textColor="#ffffffff"
                        android:text="9"/>
                    <TextView
                        android:id="@+id/t03"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textSize="20sp"
                        android:textColor="#ffffffff"
                        android:text="10"/>
                    <TextView
                        android:id="@+id/t04"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textSize="20sp"
                        android:textColor="#ffffffff"
                        android:text="11"/>
                    <TextView
                        android:id="@+id/t05"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textSize="20sp"
                        android:textColor="#ffffffff"
                        android:text="12"/>
                    <TextView
                        android:id="@+id/t06"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textSize="20sp"
                        android:textColor="#ffffffff"
                        android:text="13"/>
                    <TextView
                        android:id="@+id/t07"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textSize="20sp"
                        android:textColor="#ffffffff"
                        android:text="14"/>
                    <TextView
                        android:id="@+id/t08"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textSize="20sp"
                        android:textColor="#ffffffff"
                        android:text="15"/>
                    <TextView
                        android:id="@+id/t09"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textSize="20sp"
                        android:textColor="#ffffffff"
                        android:text="16"/>
                    <TextView
                        android:id="@+id/t0A"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textSize="20sp"
                        android:textColor="#ffffffff"
                        android:text="17"/>
                    <TextView
                        android:id="@+id/t0B"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textSize="20sp"
                        android:textColor="#ffffffff"
                        android:text="18"/>
                    <TextView
                        android:id="@+id/t0C"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textSize="20sp"
                        android:textColor="#ffffffff"
                        android:text="19"/>
                    <TextView
                        android:id="@+id/t0D"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textSize="20sp"
                        android:textColor="#ffffffff"
                        android:text="20"/>
                    <TextView
                        android:id="@+id/t0E"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textSize="20sp"
                        android:textColor="#ffffffff"
                        android:text="21"/>
                    <TextView
                        android:id="@+id/t0F"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textSize="20sp"
                        android:textColor="#ffffffff"
                        android:text="22"/>
                    <TextView
                        android:id="@+id/ttt"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textSize="20sp"
                        android:textColor="#ffffffff"
                        android:text="23"/>
                </LinearLayout>
                <RelativeLayout
                    android:id="@+id/SBrelative1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/SBrelative2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/SBrelative3"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/SBrelative4"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/SBrelative5"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/SBrelative6"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/SBrelative7"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                </RelativeLayout>
            </LinearLayout>
        </ScrollView>


    </LinearLayout>
</FrameLayout>